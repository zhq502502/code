#nav { background:none repeat scroll 0 0 #0f6691; }
#nav .ym-hlist {  margin: 0 auto; max-width: 80em; background:#0f6691; }
.navtop a{ color:#FFFFFF;}
.navtop a:hover, .navtop a:focus {
    background-color: #880000;
    color: #FFFFFF;
}



.navbottom { margin-left:20px; height:32px; line-height:32px; vertical-align: bottom; }
.box .error { height: 20px; vertical-align: middle; line-height: 20px; }

#email_list{width:auto; min-width:200px;list-style:none;border:1px solid #74c9e6;-moz-border-radius:0 0 2px 2px;border-radius:2px; position:fixed; margin-left: 180px; background:#fff;display:none; z-index:1000;}
#email_list li{width:100%;height:30px;line-height:30px;text-indent:10px;cursor:pointer;overflow:hidden;}
#email_list li.first_li{cursor:default;}
#email_list .current{background:#baeafb;width:90%;}
#main .ym-column { margin-top: -20px; }
#main .ym-col1 { width: 20%; border-left: 1px solid #ccc; min-height:500px;} /* left column */
#main .ym-col3 { margin-left: 20%; margin-right: 0; border-right: 1px solid #ccc; min-height:500px;} /* center column */

#main .ym-vlist {margin: 0; padding: 0; background-color: #fff; min-height: 420px; height:auto; }

#menu-main { margin-left:60px;margin-top:40px; padding:0; height:auto; width:50%; } 
#menu-main ul, #menu-main li{ list-style: none outside none;  margin-top:5px; }
#menu-main ul li .current{ font-weight: bold; color: #000; }

.ym-form input[type=text]{ width:40%; height:28px; line-height:28px; }
.ym-searchform {margin:-5px 0 5px 0;padding:0;}
.ym-searchform .ym-searchbutton {
 background-color: #fea000;
 color:#fff;
 font-weight: bold;
}
.ym-searchform .ym-searchfield {height:22px; line-height:22px;}
#calendar_content { margin-top:20px; }
#main #div1 {margin-top:20px; }

form .error {  background: transparent; color: #f00; }
.ym-columnar .ym-fbox-text label, .ym-columnar .ym-fbox-select label { display: inline; float: left; width: 20%; margin-left: 10px; }
.indexcloud {margin: 0 auto; padding:0; width:960px; border:1px solid #ccc;}
.indexcloud .box{padding-top: 68px; padding-bottom: 68px;  text-align:center; vertical-align: middle;}
#main a:hover, #main a:focus{ background: transparent; color:#000;}
#calendar_content hr {margin:0; padding:0;}
#calendar_tab h6 {margin-left:-5px; }
.tabNav { margin: 48px 0 0; text-align: left; }
.tabNav a.tabberactive:link, .tabNav a.tabberactive:visited {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background-color: #FFFFFF;
    border-color: #CACACA #CACACA #FFFFFF;
    border-style: dotted dotted solid;
    border-width: 1px;
    color: #474747;
    height: 21px;
    line-height: 21px;
    vertical-align: middle;
    text-align: center;
}
.tabNav a {
    background: none repeat scroll 0 0 #EEEEEE;
    border: 1px dotted #CACACA;
    color: #474747;
    display: inline-block;
    font-size: 12px;
    height: 21px;
    line-height: 21px;
    margin-left: 5px;
    margin-top: -1px;
    padding-top: 2px;
    text-decoration: none;
    vertical-align: middle;
    width: 80px;
    text-align: center;
}
#manager_content hr {margin:0; padding:0;}
.ym-searchform { display: table; float:left;line-height: 2em; margin:20px 0 -2px 0; padding: 0; }
#reserve_content {margin:40px 0 0 0;}

.ym-form input[type="password"] {
    -moz-box-sizing: border-box;
    width: 40%;
    height: 28px;
    line-height: 28px;   
}
.ym-fbox-check {margin-left:-11%;}
.error {margin-left:10px; height: 25px; line-height: 25px; text-align:left; margin-left:30%; margin-bottom:5px; }
/*----------分页----------*/
DIV.sabrosus {PADDING:0;MARGIN:0px auto;TEXT-ALIGN:center; font-size:12px; height:28px; line-height:28px;}
DIV.sabrosus A {PADDING:2px 8px; BORDER:#929292 1px solid;COLOR:#000000; MARGIN-RIGHT: 2px;TEXT-DECORATION: none; background:#eef6f9;}
DIV.sabrosus A:hover {BORDER: #2b66a5 1px solid; COLOR:#FFFFFF; BACKGROUND-COLOR:#2e6ab1;}
DIV.pagination A:active {BORDER: #2b66a5 1px solid; COLOR:#FFFFFF; BACKGROUND-COLOR:#2e6ab1;}
DIV.sabrosus SPAN {margin:2px;}
DIV.sabrosus SPAN.current { BORDER: navy 1px solid; PADDING:2px 5px; FONT-WEIGHT: bold;COLOR: #fff; MARGIN-RIGHT: 2px;BACKGROUND-COLOR: #2e6ab1;}
DIV.sabrosus SPAN.disabled {BORDER: #929292 1px solid; PADDING:2px 8px; COLOR:#000000;; MARGIN-RIGHT: 2px; background:#eef6f9;}
